{
  "name": "yordles",
  "version": "0.1.1-14",
  "description": "Anothter isomorphic framework for server-rendered React apps",
  "keywords": [
    "egg",
    "egg-framework"
  ],
  "main": "index.js",
  "scripts": {
    "test": "npm run lint -- --fix && egg-bin pkgfiles && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && egg-bin pkgfiles --check && npm run cov",
    "autod": "autod",
    "pkgfiles": "egg-bin pkgfiles",
    "postinstall": "node ./scripts/postinstall.js",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate"
  },
  "dependencies": {
    "deep-extend": "^0.6.0",
    "egg": "^2.11.2",
    "egg-view-react-ssr": "^2.3.1",
    "egg-webpack": "^4.4.4",
    "egg-webpack-react": "^2.0.2",
    "fs-extra": "^7.0.0"
  },
  "devDependencies": {
    "all-contributors-cli": "^5.4.0",
    "autod": "^3.0.1",
    "autod-egg": "^1.0.0",
    "egg-bin": "^4.9.0",
    "egg-ci": "^1.9.2",
    "egg-mock": "^3.20.1",
    "eslint": "^5.6.0",
    "eslint-config-egg": "^7.1.0",
    "webstorm-disable-index": "^1.2.0"
  },
  "files": [
    "index.js",
    "app",
    "lib",
    "test",
    "config",
    "scripts",
    ".babelrc",
    ".eslintrc",
    ".eslintignore",
    "postcss.config.js",
    "webpack.config.js"
  ],
  "eslintIgnore": [
    "coverage",
    "dist"
  ],
  "ci": {
    "version": "8, 9"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "repository": "git@github.com:thonatos/Yordles.git",
  "author": "Suyi <thonatos.yang@gmail.com>",
  "license": "MIT"
}
